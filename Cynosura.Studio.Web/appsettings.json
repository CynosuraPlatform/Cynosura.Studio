{
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=cynosura.db"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information"
    }
  },
  "IdentityServer": {
    "Clients": {
      "Cynosura.Studio.Web.Ng": {
        "Profile": "SPA",
        "RedirectUri": "http://localhost:4200/authentication/login-callback",
        "LogoutUri": "http://localhost:4200/authentication/logout-callback"
      },
      "Swagger": {
        "Profile": "IdentityServerSPA",
        "RedirectUri": "/swagger/oauth2-redirect.html"
      }
    }
  },
  "Authority": "",
  "AllowedHosts": "*",
  "Cors": {
    "Origin": "http://localhost:4300"
  },
  "Nuget": {
    "FeedUrl": "https://api.nuget.org/v3/index.json",
    "Username": "",
    "Password": ""
  },
  "Templates": [
    {
      "Name": "Cynosura.Template"
    }
  ],
  "Kestrel": {
    "EndPoints": {
      "Http": {
        "Url": "http://0.0.0.0:43000"
      }
    }
  },
  "EmailSender": {
    "DefaultFrom": "admin@cynosura.dev"
  },
  "FileSystemStorage": {
    "Url": "https://localhost:42001/static",
    "Path": "wwwroot\\static"
  }
}
