main(model) ::= <<
using Cynosura.EF;
using $model.Solution.Namespace$.Core.Entities;
using Microsoft.EntityFrameworkCore;

namespace $model.Solution.Namespace$.Data.DataContextModule
{
    public class $model.Entity.Name$Module : IDbContextModule
    {
        public void CreateModel(ModelBuilder builder)
        {
            builder.Entity<$model.Entity.Name$>();
        }
    }
}

>>
